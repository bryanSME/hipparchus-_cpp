       this.nd = trio_n.dn().reciprocal();\r\n\r\n        this.sd = nd.multiply(trio_n.sn());\r\n\r\n        this.cd = nd.multiply(trio_n.cn());\r\n\r\n    }\r\n\r\n\r\n\r\n    /** Get the value of the nd function.\r\n\r\n     * @return nd(u|m)\r\n\r\n     */\r\n\r\n    public T nd() \r\n\r\n    {\r\n\r\n        return nd;\r\n\r\n    }\r\n\r\n\r\n\r\n    /** Get the value of the sd function.\r\n\r\n     * @return sd(u|m)\r\n\r\n     */\r\n\r\n    public T sd() \r\n\r\n    {\r\n\r\n        return sd;\r\n\r\n    }\r\n\r\n\r\n\r\n    /** Get the value of the cd function.\r\n\r\n     * @return cd(u|m)\r\n\r\n     */\r\n\r\n    public T cd() \r\n\r\n    {\r\n\r\n        return cd;\r\n\r\n    }\r\n\r\n\r\n\r\n}\r\n\r\n\r\n\r\n\r\n\r\n